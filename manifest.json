{
    "name": "Productivity",
    "description": "This extension will help you to focus on your work and leave the rest of the unproductive and distracting sites on us",
    "manifest_version": 3,
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "256": "256.png",
        "128": "128.png",
        "64": "64.png",
        "32": "32.png",
        "16": "16.png"

    }

}